    "@testing-library/user-event": "^13.1.9",
    "@types/bignumber.js": "^5.0.0",
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.19.9",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.5",
